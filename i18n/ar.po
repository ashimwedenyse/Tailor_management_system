#. module: tailor_management
#: model:mail.template,body_html:tailor_management.email_template_mo_done
msgid ""
"<div style=\"font-family:Arial,sans-serif; margin: 0px; padding: 20px;\">\n"
"                    <p>Hello <t t-out=\"object.partner_id.name or 'Customer'\"/>,</p>\n"
"\n"
"                    <p>\n"
"                        We are happy to inform you that your manufacturing order\n"
"                        <strong><t t-out=\"object.name\"/></strong>\n"
"                        has been completed.\n"
"                    </p>\n"
"\n"
"                    <div style=\"background-color:#f8f9fa; padding:15px; border-radius:5px;\">\n"
"                        <p><strong>Product:</strong>\n"
"                            <t t-out=\"object.product_id.display_name\"/>\n"
"                        </p>\n"
"                        <p><strong>Quantity:</strong>\n"
"                            <t t-out=\"object.product_qty\"/>\n"
"                        </p>\n"
"                        <p><strong>Delivery Date:</strong>\n"
"                            <t t-out=\"object.delivery_date\"/>\n"
"                        </p>\n"
"                    </div>\n"
"\n"
"                    <p style=\"margin-top:20px;\">\n"
"                        Thank you for choosing our tailoring services!\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr ""
"<div style=\"font-family:Arial,sans-serif; margin: 0px; padding: 20px;\">\n"
"                    <p>مرحباً <t t-out=\"object.partner_id.name or 'عميلنا العزيز'\"/>،</p>\n"
"\n"
"                    <p>\n"
"                        يسعدنا إبلاغك بأن أمر التصنيع الخاص بك\n"
"                        <strong><t t-out=\"object.name\"/></strong>\n"
"                        قد اكتمل بنجاح.\n"
"                    </p>\n"
"\n"
"                    <div style=\"background-color:#f8f9fa; padding:15px; border-radius:5px;\">\n"
"                        <p><strong>المنتج:</strong>\n"
"                            <t t-out=\"object.product_id.display_name\"/>\n"
"                        </p>\n"
"                        <p><strong>الكمية:</strong>\n"
"                            <t t-out=\"object.product_qty\"/>\n"
"                        </p>\n"
"                        <p><strong>تاريخ التسليم:</strong>\n"
"                            <t t-out=\"object.delivery_date\"/>\n"
"                        </p>\n"
"                    </div>\n"
"\n"
"                    <p style=\"margin-top:20px;\">\n"
"                        نشكرك على اختيار خدمات الخياطة لدينا!\n"
"                    </p>\n"
"                </div>"

